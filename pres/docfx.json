{
	"metadata": [
		{
			"src": [
				{
					"src": "../",
					"files": [
						"src/**.csproj"
					],
					"exclude": [
						"**/obj/**",
						"**/bin/**"
					]
				}
			],
			"dest": "../pres/api"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**"
				]
			}
		],
		"dest": "../docs",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default"
		],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false
	}
}